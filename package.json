{
  "name": "CRviz",
  "version": "0.3.1-alpha8",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.1",
    "boom": "^7.2.1",
    "classnames": "^2.2.6",
    "cryptiles": "^4.1.2",
    "css-element-queries": "^1.0.2",
    "d3": "^5.4.0",
    "d3-scale-chromatic": "^1.2.0",
    "d3-svg-annotation": "^2.3.1",
    "d3-tip": "^0.9.1",
    "debug": "^4.0.1",
    "deep-extend": "^0.5.1",
    "extend": "^3.0.2",
    "filtrex": "^0.5.4",
    "hawk": "^7.0.0",
    "http-signature": "^1.1.1",
    "lodash.debounce": "^4.0.8",
    "lunr": "^2.2.0",
    "mini.css": "^3.0.1",
    "normalize.css": "^8.0.0",
    "ramda": "^0.26.0",
    "rc": "^1.2.7",
    "react": "^16.6.3",
    "react-beautiful-dnd": "^10.0.3",
    "react-dom": "^16.6.3",
    "react-modal": "^3.5.1",
    "react-portal-tooltip": "^2.4.0",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.2.1",
    "redux-mock-store": "^1.5.1",
    "redux-observable": "^1.0.0-beta.2",
    "request": "^2.84.0",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "snyk": "^1.94.0",
    "tar-pack": "^3.4.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version PORT=5000 react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov && codecov",
    "test": "NODE_ENV=test NODE_PATH=./src nyc mocha --opts ./.mocha --exit --recursive 'src/**/*.test.js*'",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "deploy-experimental": "gh-pages -r https://github.com/rashley-iqt/CRviz -d build",
    "lint": "eslint --ext=js --ext=jsx .",
    "release": "standard-version",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js*",
      "test/**"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.9.1",
    "gh-pages": "^2.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "pre-commit": "^1.2.2",
    "react-app-rewire-eslint": "^0.2.3",
    "sinon": "^6.1.3",
    "standard-version": "^4.4.0"
  },
  "snyk": true
}
